Grammar

    0 $accept: programa $end

    1 programa: statement_block

    2 statement_block: statement statement_block
    3                | /* empty */

    4 statement: declaracao
    5          | funcao

    6 identifier: TK_IDENTIFIER

    7 declaracao: tipo identifier ';'
    8           | tipo identifier '[' LIT_INTEGER ']' ';'

    9 tipo: KW_INT
   10     | KW_CHAR

   11 funcao: tipo identifier '(' lista_parametros ')' bloco

   12 lista_parametros: parametro
   13                 | parametro ',' lista_parametros
   14                 | /* empty */

   15 parametro: tipo identifier

   16 bloco: '{' bloco_comandos '}'
   17      | comando ';'

   18 bloco_comandos: comando ';' bloco_comandos
   19               | comando

   20 comando: atribuicao
   21        | fluxo
   22        | KW_READ identifier
   23        | KW_PRINT aritmetica
   24        | KW_PRINT string
   25        | KW_PRINT '(' string ')'
   26        | KW_RETURN expressao
   27        | /* empty */

   28 atribuicao: identifier '=' expressao
   29           | identifier '[' expressao ']' '=' expressao

   30 expressao: identifier
   31          | identifier '[' expressao ']'
   32          | literal
   33          | logica
   34          | aritmetica

   35 literal: LIT_CHAR
   36        | LIT_STRING
   37        | LIT_FLOAT
   38        | LIT_FALSE
   39        | LIT_TRUE
   40        | LIT_INTEGER

   41 string: LIT_STRING

   42 aritmetica: '(' aritmetica ')'
   43           | expressao '+' expressao
   44           | expressao '-' expressao
   45           | expressao '*' expressao
   46           | expressao '/' expressao

   47 logica: '(' logica ')'
   48       | expressao OPERATOR_AND expressao
   49       | expressao OPERATOR_OR expressao
   50       | expressao OPERATOR_LE expressao
   51       | expressao OPERATOR_GE expressao
   52       | expressao OPERATOR_EQ expressao
   53       | expressao OPERATOR_NE expressao

   54 fluxo: KW_IF '(' expressao ')' KW_THEN bloco
   55      | KW_IF '(' expressao ')' KW_THEN bloco KW_ELSE bloco
   56      | KW_WHILE '(' expressao ')' bloco


