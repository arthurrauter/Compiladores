Grammar

    0 $accept: programa $end

    1 programa: statement_block

    2 statement_block: statement statement_block
    3                | /* empty */

    4 statement: declaracao
    5          | funcao

    6 identifier: TK_IDENTIFIER

    7 declaracao: tipo identifier ';'
    8           | tipo identifier '[' LIT_INTEGER ']' ';'

    9 tipo: KW_INT
   10     | KW_CHAR

   11 funcao: tipo identifier '(' lista_parametros ')' bloco

   12 lista_parametros: parametro
   13                 | parametro ',' lista_parametros
   14                 | /* empty */

   15 parametro: tipo identifier

   16 bloco: '{' bloco_comandos '}'
   17      | comando ';'

   18 bloco_comandos: comando ';' bloco_comandos
   19               | comando

   20 comando: atribuicao
   21        | fluxo
   22        | KW_READ identifier
   23        | KW_PRINT aritmetica
   24        | KW_PRINT LIT_STRING
   25        | KW_RETURN expressao
   26        | /* empty */

   27 atribuicao: identifier '=' expressao
   28           | identifier '[' expressao ']' '=' expressao

   29 expressao: identifier
   30          | identifier '[' expressao ']'
   31          | literal
   32          | logica
   33          | aritmetica

   34 literal: LIT_CHAR
   35        | LIT_STRING
   36        | LIT_FLOAT
   37        | LIT_FALSE
   38        | LIT_TRUE
   39        | LIT_INTEGER

   40 aritmetica: '(' aritmetica ')'
   41           | expressao '+' expressao
   42           | expressao '-' expressao
   43           | expressao '*' expressao
   44           | expressao '/' expressao

   45 logica: '(' logica ')'
   46       | expressao OPERATOR_AND expressao
   47       | expressao OPERATOR_OR expressao
   48       | expressao OPERATOR_LE expressao
   49       | expressao OPERATOR_GE expressao
   50       | expressao OPERATOR_EQ expressao
   51       | expressao OPERATOR_NE expressao

   52 fluxo: KW_IF '(' expressao ')' KW_THEN bloco
   53      | KW_IF '(' expressao ')' KW_THEN bloco KW_ELSE bloco
   54      | KW_WHILE '(' expressao ')' bloco


